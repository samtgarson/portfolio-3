{
  "version": 2,
  "name": "portfolio-3",
  "alias": [
    "samgarson.com",
    "www.samgarson.com",
    "samgarson.xyz"
  ],
  "env": {
    "FOURSQUARE_TOKEN": "@foursquare-token",
  },
  "build": {
    "env": {
      "VUE_APP_CHECKIN_URL": "/api/where"
    }
  },
  "builds": [
    {
      "src": "./app/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "./api/where/index.js",
      "use": "@now/node"
    },
    }
  ],
  "routes": [
    {
      "src": "^/api/where",
      "dest": "/api/where/index.js"
    },
    {
      "src": "^/(.*)",
      "dest": "/app/$1"
    }
  ]
}
